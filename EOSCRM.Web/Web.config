        <configuration>
          <renderNotASPAJAXDateTime/>
          <objectExtendPrototype/>
          <appCodeQualifiedFullName/>
          <allowNumberBooleanAsString/>
          <sessionStateDefaultNone/>
          <includeMsPrototype/>
          <renderDateTimeAsString/>
          <noUtcTime/>
          <renderJsonCompliant/>
          <useSimpleObjectNaming/>
        </configuration>
        
